CAAA,IAAA;;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,MAAA,GAAA,CAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,YAAA,GAAA,EAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,cAAA,EAAA,IAAA,EAAA;AACA,YAAA,iBAAA,EAAA;AADA,WAAA;AAIA,SARA,MAQA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,EAAA,IAAA,EAAA;AACA,YAAA,iBAAA,EAAA;AADA,WAAA;;AAGA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,YAAA,IAAA,EAAA,MADA;AAEA,YAAA,KAAA,EAAA;AACA,cAAA,KAAA,EAAA,KAAA,CAAA,KADA;AAEA,cAAA,GAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAFA;AAFA,WAAA;AAOA;AACA,OA7BA,EA8BA,KA9BA,CA8BA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAhCA;AAiCA,KA/CA;AAiDA;AACA,IAAA,IAlDA,kBAkDA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,CAAA;AACA,WAAA,IAAA,CAAA,YAAA,GAAA,EAAA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AAvDA;AAZA,CAAA","sourcesContent":["<template>\n  <el-card style=\"margin-left:500px;margin-right:500px;margin-top:100px\">\n    <p style=\"margin-left:120px\">上架商品</p>\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\n      <el-form-item label=\"商品名称: \">\n        <el-input v-model=\"form.cname\" placeholder=\"这里输入商品名称\"></el-input>\n      </el-form-item>\n\n      <el-form-item label=\"价格/元: \">\n        <el-input-number v-model=\"form.cprice\" :precision=\"2\"></el-input-number>\n      </el-form-item>\n\n      <el-form-item label=\"详细信息: \">\n        <el-input type=\"textarea\" :rows=\"5\" placeholder=\"这里输入联系方式、商品描述\" v-model=\"form.cinformation\"></el-input>\n      </el-form-item>\n\n      <el-form-item>\n        <el-button type=\"success\" @click=\"onSubmit()\">提交</el-button>\n        <el-button type=\"warning\" @click=\"back()\">取消并返回</el-button>\n      </el-form-item>\n    </el-form>\n  </el-card>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      uname:this.$route.query.uname,\n      form: {\n        uno: this.$route.query.uno,\n        cname: \"\",\n        cprice: \"\",\n        cinformation: \"\",\n      }\n    };\n  },\n  methods: {\n    //检查后发送上架商品请求\n    onSubmit() {\n      if (this.form.cname.trim() === \"\") {\n        this.$alert(\"商品名字不能为空\", \"提示\", {\n          confirmButtonText: \"确定\"\n        });\n        return false;\n      } else if (this.form.cinformation.trim() === \"\") {\n        this.$alert(\"你详情至少打一个字鸭\", \"提示\", {\n          confirmButtonText: \"确定\"\n        });\n        return false;\n      }\n      this.$axios({\n        headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\n        method: \"POST\",\n        url: \"api/addCommodity\",\n        data: this.$qs.stringify(this.form)\n      })\n        .then(res => {\n          //获取response信息\n          console.log(res.data);\n          if (res.data == false) {\n            this.form.cname = \"\";\n            this.form.cprice = 0;\n            this.form.cinformation = \"\";\n            this.$alert(\"商品发布失败，请重新输入\", \"提示\", {\n              confirmButtonText: \"确定\"\n            });\n            \n          } else {\n            this.$alert(\"商品发布成功\", \"提示\", {\n              confirmButtonText: \"确定\"\n            });\n            this.$router.push({\n              name: \"Home\",\n              query: {\n                uname: this.uname,\n                uno: this.form.uno\n              }\n            });\n          }\n        })\n        .catch(Error => {\n          console.log(Error);\n        });\n    },\n\n    //返回\n    back() {\n      this.form.cname = \"\";\n      this.form.cprice = 0;\n      this.form.cinformation = \"\";\n      this.$router.go(-1);\n    }\n  }\n};\n</script>"],"sourceRoot":"src/components"}]}