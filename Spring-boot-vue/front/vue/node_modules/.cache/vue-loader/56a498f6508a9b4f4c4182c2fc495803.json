e>\n\n<script>\nexport default {\n  name: \"login\",\n  data() {\n    return {\n      loginForm: {\n        u_no: \"\",\n        u_pwd: \"\"\n      }\n    };\n  },\n  methods: {\n    toRegister() {\n      this.$router.push(\"/register\");\n    },\n    handleLogin() {\n      this.$axios({\n        headers: { \"Content-Type\": \"application/x-www-form-urlencoded\" },\n        method: \"POST\",\n        url: \"api/checkpwd\",\n        data: this.$qs.stringify(this.loginForm)\n      })\n        .then(res => {\n          //获取response信息\n          console.log(res.data);\n          if (res.data == \"false\") {\n            (this.loginForm.u_no = \"\"),\n              (this.loginForm.u_pwd = \"\"),\n              this.$alert(\"用户名密码错误\", \"提示\", {\n                confirmButtonText: \"确定\"\n              });\n          } else {\n            // this.$router.push(\"/home\");\n\n            this.$router.push({\n              name: \"Home\",\n              query: {\n                username: this.loginForm.username,\n                userid: res.data\n              }\n            });\n          }\n        })\n        .catch(Error => {\n          console.log(Error);\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n#container {\n  width: 100%;\n  height: 100%;\n  background-image: url(\"../assets/backgroud2.png\");\n  background-size: 100%;\n  background-position: center;\n}\n</style>"]}]}